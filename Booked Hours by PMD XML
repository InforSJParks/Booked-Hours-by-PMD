						<sqlQuery name="SQL1" dataSource="AdvRepData">
			<sqlText>SELECT LAZY.PER_CODE, PER_UDFCHAR01, PER_TRADE
FROM



(SELECT PER_CODE
FROM


(SELECT PER_CODE, PER_UDFCHAR01, PER_NOTUSED
FROM R5PERSONNEL
WHERE (PER_UDFCHAR01 IN (#PROMPTMANY('pmd')#) OR PER_UDFCHAR01 = #PROMPT('DEPT')# OR (#PROMPT('DEPT')# = '*' AND PER_UDFCHAR01 IS NOT NULL)) 
AND PER_NOTUSED = '-'
)


MINUS
SELECT BOO_PERSON
FROM


(SELECT BOO_PERSON, BOO_DATE
FROM R5BOOKEDHOURS
WHERE 
TRUNC(BOO_DATE) BETWEEN trunc(to_date(#prompt('Start Date')#, 'YYYY-MM-DD')) and trunc(to_date(#prompt('End Date')#, 'YYYY-MM-DD')))) "LAZY"


INNER JOIN R5PERSONNEL ON LAZY.PER_CODE = R5PERSONNEL.PER_CODE</sqlText>
		<mdProjectedItems><mdProjectedItem name="PER_CODE"/><mdProjectedItem name="PER_UDFCHAR01"/><mdProjectedItem name="PER_TRADE"/></mdProjectedItems></sqlQuery></source>
						<selection><dataItem name="PER_CODE"><expression>[SQL1].[PER_CODE]</expression></dataItem><dataItem name="Employee"><expression>REPGETDESC('EN', 'PERS', [PER_CODE], NULL, NULL)</expression></dataItem><dataItem name="PER_UDFCHAR01" label="PMD"><expression>[SQL1].[PER_UDFCHAR01]</expression></dataItem><dataItem name="PER_TRADE"><expression>[SQL1].[PER_TRADE]</expression></dataItem></selection>
					</query><query name="PMD LOV">
			<source>
				
			<sqlQuery name="SQL2" dataSource="AdvRepData">
			<sqlText>SELECT CASE #prompt('DEPT')#
WHEN 'PK-MAINT' THEN
	obj_code
WHEN '*' THEN
	'All PMDs'
ELSE
	'None'
END
	as obj_code FROM r5objects
	WHERE (obj_code LIKE 'PMD _' AND #prompt('DEPT')# in ('PK-MAINT', '*'))
	OR (#prompt('DEPT')# NOT in ('PK-MAINT', '*'))
	</sqlText>
		<mdProjectedItems><mdProjectedItem name="OBJ_CODE"/></mdProjectedItems></sqlQuery></source>
			<selection><dataItem name="OBJ_CODE"><expression>[SQL2].[OBJ_CODE]</expression></dataItem></selection>
		</query><query name="UDF CrossTab"><source><sqlQuery name="SQL3" dataSource="AdvRepData">
			<sqlText>SELECT row_number() over ( order by boo_event ) as BNum, EVT_CODE, EVT_TYPE,  BOO_DATE, BOO_HOURS, BOO_EVENT,  BOO_PERSON, BOO_MRC, PER_CODE, PER_UDFCHAR01, PER_NOTUSED, PER_TRADE


FROM R5PERSONNEL
join R5BOOKEDHOURS on BOO_PERSON = PER_CODE
join R5EVENTS on EVT_CODE = BOO_EVENT

where  EVT_TYPE IN ('JOB','PPM')
AND EVT_PRIORITY &lt;&gt; '*'
AND (PER_UDFCHAR01 IN (#PROMPTMANY('pmd')#) OR PER_UDFCHAR01 = #PROMPT('DEPT')# OR (#PROMPT('DEPT')# = '*' AND PER_UDFCHAR01 IS NOT NULL)) </sqlText>
		<mdProjectedItems><mdProjectedItem name="BNUM"/><mdProjectedItem name="EVT_CODE"/><mdProjectedItem name="EVT_TYPE"/><mdProjectedItem name="BOO_DATE"/><mdProjectedItem name="BOO_HOURS"/><mdProjectedItem name="BOO_EVENT"/><mdProjectedItem name="BOO_PERSON"/><mdProjectedItem name="BOO_MRC"/><mdProjectedItem name="PER_CODE"/><mdProjectedItem name="PER_UDFCHAR01"/><mdProjectedItem name="PER_NOTUSED"/><mdProjectedItem name="PER_TRADE"/></mdProjectedItems></sqlQuery></source><selection><dataItem name="BOO_DATE"><expression>[SQL3].[BOO_DATE]</expression></dataItem><dataItem name="Hours Worked" aggregate="total"><expression>[SQL3].[BOO_HOURS]</expression></dataItem><dataItem name="BOO_PERSON"><expression>[SQL3].[BOO_PERSON]</expression></dataItem><dataItem name="Person"><expression>REPGETDESC ('EN', 'PERS', [BOO_PERSON], NULL, NULL)</expression></dataItem><dataItem name="Date"><expression>to_char([BOO_DATE], 'Day MM/DD')</expression></dataItem><dataItemEdgeSummary solveOrder="1" refDataItem="Date" aggregateMethod="total" name="Total(Date)" label="Total"/><dataItem name="PER_NOTUSED"><expression>[SQL3].[PER_NOTUSED]</expression></dataItem><dataItem name="PER_UDFCHAR01"><expression>[SQL3].[PER_UDFCHAR01]</expression></dataItem><dataItem name="BNUM"><expression>[SQL3].[BNUM]</expression></dataItem><dataItem name="PER_TRADE"><expression>[SQL3].[PER_TRADE]</expression></dataItem></selection><detailFilters><detailFilter><filterExpression>TRUNC([BOO_DATE]) BETWEEN  trunc(to_date(#prompt('Start Date')#, 'YYYY-MM-DD')) AND trunc(to_date(#prompt('End Date')#, 'YYYY-MM-DD'))</filterExpression></detailFilter></detailFilters></query><query name="Full-Time Hours"><source><sqlQuery name="SQL4" dataSource="AdvRepData">
			<sqlText>SELECT PER_CODE, PER_TRADE, BOO_DATE, BOO_HOURS, PER_TERMINATEDDATE, row_number() over ( order by boo_event ) as BNum

FROM R5BOOKEDHOURS
join R5EVENTS on boo_event = evt_code
FULL OUTER join R5PERSONNEL on PER_CODE = BOO_PERSON

where (PER_UDFCHAR01 IN (#PROMPTMANY('pmd')#) OR PER_UDFCHAR01 = #PROMPT('DEPT')# OR (#PROMPT('DEPT')# = '*' AND PER_UDFCHAR01 IS NOT NULL)) 
AND PER_NOTUSED = '-'
AND EVT_PRIORITY &lt;&gt; '*'

UNION

SELECT PER_CODE, PER_TRADE, NULL, NULL, PER_TERMINATEDDATE, NULL
FROM R5PERSONNEL
WHERE (PER_UDFCHAR01 IN (#PROMPTMANY('pmd')#) OR PER_UDFCHAR01 = #PROMPT('DEPT')# OR (#PROMPT('DEPT')# = '*' AND PER_UDFCHAR01 IS NOT NULL))
AND PER_NOTUSED = '-'</sqlText>
		<mdProjectedItems><mdProjectedItem name="PER_CODE"/><mdProjectedItem name="PER_TRADE"/><mdProjectedItem name="BOO_DATE"/><mdProjectedItem name="BOO_HOURS"/><mdProjectedItem name="PER_TERMINATEDDATE"/><mdProjectedItem name="BNUM"/></mdProjectedItems></sqlQuery></source><selection><dataItem name="Date Num"><expression>((cast(_days_between(?End Date?,1900-01-01)/7,integer)*5)+if(_day_of_week(?End Date?,1)&gt;5) then (4) else (mod(_days_between(?End Date?,1900-01-01),7))+1)
-
((cast(_days_between(?Start Date?,1900-01-01)/7,integer)*5)+if(_day_of_week(?Start Date?,1)&gt;5) then (4) else (mod(_days_between(?Start Date?,1900-01-01),7))+1)
+
(if(_day_of_week(?Start Date?,1)&lt;6) then (1) else (0))</expression></dataItem><dataItem name="Date"><expression>to_char([SQL4].[BOO_DATE],'Day MM/DD')</expression></dataItem><dataItem name="Expected Hours"><expression>[Date Num] * 8</expression></dataItem><dataItem name="Full Expected Total"><expression>COUNT ( distinct

IF ([SQL4].[PER_TRADE] NOT LIKE '%PT%')
THEN ([SQL4].[PER_CODE])
ELSE (NULL)

) * [Expected Hours]</expression></dataItem><dataItem name="PER_CODE"><expression>[SQL4].[PER_CODE]</expression></dataItem><dataItem name="Total Full Hours" aggregate="total"><expression>TOTAL (

IF ([SQL4].[PER_TRADE] NOT LIKE '%PT%')
THEN ([SQL4].[BOO_HOURS])
ELSE (NULL)

)</expression></dataItem><dataItem name="Percantage of Hours"><expression>[Total Full Hours]/[Full Expected Total]</expression></dataItem><dataItem name="PER_TERMINATEDDATE"><expression>[SQL4].[PER_TERMINATEDDATE]</expression></dataItem></selection><detailFilters><detailFilter><filterExpression>TRUNC([SQL4].[BOO_DATE]) BETWEEN  trunc(to_date(#prompt('Start Date')#, 'YYYY-MM-DD')) AND trunc(to_date(#prompt('End Date')#, 'YYYY-MM-DD')) OR [SQL4].[BOO_DATE] IS NULL</filterExpression></detailFilter></detailFilters></query><query name="Departments">
